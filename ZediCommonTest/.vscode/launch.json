{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/output/TestRunner",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
